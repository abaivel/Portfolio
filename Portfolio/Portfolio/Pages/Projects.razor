@page "/projects"
@using MudBlazor

<PageTitle>Baivel - Projects</PageTitle>

<div style="width:100%;height:100%" id="fond">
	<div style="height:100%;margin-top:-64px;" id="home-background">
		<div style="background-color: #717486bf;">
			<MudItem Class="pl-20" Style="padding-top:100px;height:max-content;padding-bottom: 36px;">
				<MudText Typo="Typo.h3" Style="font-family:'Cascadia Code'" Color="Color.Secondary" Align="Align.Center">Projects</MudText>
			</MudItem>
			<div style="background-color:white;margin-left:10px;margin-right:10px;border-radius:15px;padding:10px">
				<MudTabs Outlined="true" Position="Position.Left" Rounded="true" Border="true"
						 ApplyEffectsToContainer="true" PanelClass="pa-6">
					<MudTabPanel Text="Portfolio">
						<PanelProject Title="Portfolio"></PanelProject>
					</MudTabPanel>
					<MudTabPanel Text="4X Game - JEE">
						<PanelProject Title="4X Game - JEE"></PanelProject>
					</MudTabPanel>
					<MudTabPanel Text="HedgeShop - React.js & Django">
						<PanelProject Title="HedgeShop - React.js & Django"></PanelProject>
					</MudTabPanel>
					<MudTabPanel Text="LBA Proprete Website - HTML, CSS & PHP">
						<PanelProject Title="LBA Proprete Website - HTML, CSS & PHP"></PanelProject>
					</MudTabPanel>
					<MudTabPanel Text="LBA Proprete Android Application - Java & Kotlin">
						<PanelProject Title="LBA Proprete Android Application - Java & Kotlin"></PanelProject>
					</MudTabPanel>
					<MudTabPanel Text="Jeunes 6.4 - HTML, CSS & PHP">
						<PanelProject Title="Jeunes 6.4 - HTML, CSS & PHP"></PanelProject>
					</MudTabPanel>
					<MudTabPanel Text="Level 1 - Java & javafx">
						<PanelProject Title="Level 1 - Java & javafx"></PanelProject>
					</MudTabPanel>

				</MudTabs>
			</div>
			<br />
		</div>
	</div>
</div>


